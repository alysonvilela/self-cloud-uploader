{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "build:bun": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/index.tsx"
  },
  "dependencies": {
    "@uppy/aws-s3": "^5.0.1",
    "@uppy/react": "^5.0.3",
    "@uppy/tus": "^5.0.1",
    "bun-plugin-tailwind": "^0.0.15",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.1.11",
    "use-tus": "^0.8.3"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
