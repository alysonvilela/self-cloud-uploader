{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "acme",
  "private": true,
  "scripts": {
    "compose:up": "docker compose up -d",
    "compose:status": "docker compose ps",
    "compose:down": "docker compose down",
    "lint": "trunk check",
    "format": "trunk fmt",
    "dev": "bun --watch src/app.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/app.ts",
    "start": "bun src/app.ts",
    "prod": "./server"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.892.0",
    "@aws-sdk/s3-request-presigner": "3.892.0",
    "elysia": "^1.0.0",
    "@elysiajs/cors": "^1.0.0",
    "elysia-autoload": "^1.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@trunkio/launcher": "1.3.4",
    "@types/node": "24.5.2",
    "bun-types": "latest",
    "dotenv": "17.2.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "@types/bun": "latest"
  }
}
